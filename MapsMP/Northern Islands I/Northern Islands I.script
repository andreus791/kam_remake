  LocsRight.Y[12] := 53;
  LocsRight.Dir[12] := 6;

  LocsRight.X[13] := 180;
  LocsRight.Y[13] := 17;
  LocsRight.Dir[13] := 4;

  LocsRight.X[14] := 164;
  LocsRight.Y[14] := 59;
  LocsRight.Dir[14] := 7;

  for I := 0 to 14 do
  begin
    LocsLeft.SoldiersType[I] := -1;
    LocsTop.SoldiersType[I] := -1;
    LocsRight.SoldiersType[I] := -1;
    LocsLeft.CanBeTaken[I] := True;
    LocsTop.CanBeTaken[I] := True;
    LocsRight.CanBeTaken[I] := True;
  end;

end;


//Save info about destroyed house

procedure OnHouseDestroyed(aHouseID: Integer; aDestroyerIndex: Integer);
begin
  if (States.HouseOwner(aHouseID) = 2)
  and (aDestroyerIndex <> -1)
  and (States.HouseType(aHouseID) = 17) then
  begin 
    SetLength(RebuildType, RebuildCount + 1);
    SetLength(RebuildX, RebuildCount + 1);
    SetLength(RebuildY, RebuildCount + 1);
    RebuildType[RebuildCount] := States.HouseType(aHouseID); 
    RebuildX[RebuildCount] := States.HousePositionX(aHouseID); 
    RebuildY[RebuildCount] := States.HousePositionY(aHouseID); 
    RebuildCount := RebuildCount + 1; 
  end;
end;


//AI will try to rebuild towers

procedure Rebuild;
begin
  for J := 0 to RebuildCount - 1 do
  begin
    if RebuildType[J] <> - 1 then
    begin
      Actions.PlanAddHouse(2, RebuildType[J], RebuildX[J], RebuildY[J]);
      if Actions.PlanAddHouse(2, RebuildType[J], RebuildX[J], RebuildY[J]) then
        RebuildType[J] := -1; 
    end; 
  end;
end;


//Unlimited fish ;D

procedure AddFish;
var
  C1, C2: Integer;
begin
  C1 := 0;
  C2 := 0;
  if States.PlayerEnabled(0) then
    C1 := States.StatResourceProducedCount(0, 27);
  if States.PlayerEnabled(1) then
    C2 := States.StatResourceProducedCount(1, 27);
  if Fish - (C1 + C2) <= 20 then
  begin
    Actions.GiveAnimal(31, 86, 171);
    Actions.GiveAnimal(31, 87, 171);
    Actions.GiveAnimal(31, 85, 171);
    Fish := Fish + 30; 
    Exit;
  end;
end;


//Attacks

function Delay: Integer;
begin
  case AttacksCounter of
    0..4:   Result := (12 + States.KaMRandomI(4)) * 600;
    5..8:   Result := (15 + States.KaMRandomI(6)) * 600;
    9..16:  Result := (20 + States.KaMRandomI(6)) * 600
    else    Result := (30 + States.KaMRandomI(6)) * 600;
  end;
end;


function AttackersCount: Integer;
begin
  case States.MissionDifficulty of

    mdEasy1: case AttacksCounter of
               0..4:  Result := 3;
               5..8:  Result := 5;
               9..16: Result := 7
               else   Result := 8;
             end;

    mdNormal: case AttacksCounter of
                0..4:  Result := 4;
                5..8:  Result := 6;
                9..16: Result := 8
                else   Result := 9;
              end;

    mdHard1: case AttacksCounter of
               0..4:  Result := 5;
               5..8:  Result := 7;
               9..16: Result := 9
               else   Result := 11;
             end;

    mdHard2: case AttacksCounter of
               0..4:  Result := 6;
               5..8:  Result := 8;
               9..16: Result := 10
               else   Result := 12;
             end;

    mdHard3: case AttacksCounter of
               0..4:  Result := 6;
               5..8:  Result := 9;
               9..16: Result := 12
               else   Result := 15;
             end;

  end;
end;


function Balancer: Single;
begin
  case States.MissionDifficulty of
    mdEasy1:  Result := 0.75;
    mdNormal: Result := 1;
    mdHard1:  Result := 1.15;
    mdHard2:  Result := 1.25;
    mdHard3:  Result := 1.5;
  end;
end;


function SoldiersType: Integer;
begin
  case AttacksCounter of
    0..4: begin
            Result := States.KaMRandomI(8) + 15;

            if Result = 17 then
              Result:= 20;

            if Result = 18 then
              Result:= 15;

            if Result = 22 then
              Result:= 21;

          end;

    5..10: begin
             Result := States.KaMRandomI(8) + 15;

             if Result = 17 then
               Result:= 20;

             if Result = 18 then
               Result:= 16;

             if Result = 22 then
               Result:= 21;
            end;

    11..15: begin
              Result := States.KaMRandomI(8) + 15;

              if Result = 17 then
                Result:= 20;

              if Result = 18 then
                Result:= 16;

              if Result = 19 then
                Result:= 21;
            end;

    16..18: begin
              Result := States.KaMRandomI(8) + 15;

              if Result = 17 then
                Result:= 20;

              if Result = 18 then
                Result:= 16;

              if Result = 19 then
                Result:= 20;
            end;

    19..21: begin
              Result := States.KaMRandomI(8) + 15;

              if Result = 17 then
                Result:= 16;

              if Result = 18 then
                Result:= 16;

              if Result = 19 then
                Result:= 22;
            end

    else
    begin
      Result := States.KaMRandomI(8) + 15;

      if Result = 17 then
        Result:= 22;

      if Result = 18 then
        Result:= 16;

      if Result = 19 then
        Result:= 22;
    end;
  end;
end;


procedure CheckLocs;
begin

  for I := 0 to 14 do
  begin
    LocsLeft.CanBeTaken[I] := (not States.PlayerDefeated(7))
    and (States.StatArmyCount(7) > Round(50/Balancer));
    LocsRight.CanBeTaken[I] := (not States.PlayerDefeated(6))
    and (States.StatArmyCount(6) > Round(50/Balancer));
  end;

  for J := 0 to 7 do
    LocsTop.CanBeTaken[J] := (not States.PlayerDefeated(5))
    and (States.StatArmyCount(5) > Round(50/Balancer));

  for J := 8 to 13 do
    LocsTop.CanBeTaken[J] := (not States.PlayerDefeated(4))
    and (States.StatArmyCount(4) > Round(50/Balancer));

  LocsTop.CanBeTaken[14] := (not States.PlayerDefeated(3))
  and (States.StatArmyCount(3) > Round(50/Balancer));

end;


procedure Attack;
var
  Loc: Integer;
begin

  AttacksCounter := AttacksCounter + 1;
  AttackDelay := Delay;
  CheckLocs;

  for I := 0 to 14 do
  begin
    LocsLeft.SoldiersType[I] := -1;
    LocsTop.SoldiersType[I] := -1;
    LocsRight.SoldiersType[I] := -1;
  end;

  if not States.PlayerDefeated(5) then
    for I := 0 to AttackersCount - 1 do
    begin
      repeat
        Loc := States.KaMRandomI(15)
      until
        LocsTop.SoldiersType[Loc] = -1;
      LocsTop.SoldiersType[Loc] := SoldiersType;
      if LocsTop.CanBeTaken[Loc] then
        Actions.GiveGroup(8, LocsTop.SoldiersType[Loc], LocsTop.X[Loc], LocsTop.Y[Loc], LocsTop.Dir[Loc], 12, 4);
    end;

  if AttacksCounter <= 2 then
    Actions.GiveGroup(8, 14, 32, 65, 2, Utils.CeilTo(100 * Balancer, 20), 10);

  if Utils.InRangeI(AttacksCounter, 3, 4) then
    Actions.GiveGroup(8, 27, 32, 65, 2, Utils.CeilTo(85 * Balancer, 20), 10);

  if Utils.InRangeI(AttacksCounter, 5, 6) then
    Actions.GiveGroup(8, 21, 32, 65, 2, Utils.CeilTo(60 * Balancer, 20), 8);

  if AttacksCounter > 6 then
    if not States.PlayerDefeated(6) then
      for I := 0 to AttackersCount - 1 do
      begin
        repeat
          Loc := States.KaMRandomI(15)
        until
          LocsRight.SoldiersType[Loc] = -1;
        LocsRight.SoldiersType[Loc] := SoldiersType;
        if LocsRight.CanBeTaken[Loc] then
          Actions.GiveGroup(9, LocsRight.SoldiersType[Loc], LocsRight.X[Loc], LocsRight.Y[Loc], LocsRight.Dir[Loc], 12, 4);
      end;

  if AttacksCounter > 12 then
    if not States.PlayerDefeated(7) then
      for I := 0 to AttackersCount - 1 do
      begin
        repeat
          Loc := States.KaMRandomI(15)
        until
          LocsLeft.SoldiersType[Loc] = -1;
        LocsLeft.SoldiersType[Loc] := SoldiersType;
        if LocsLeft.CanBeTaken[Loc] then
          Actions.GiveGroup(10 + States.KaMRandomI(2), LocsLeft.SoldiersType[Loc], LocsLeft.X[Loc], LocsLeft.Y[Loc], LocsLeft.Dir[Loc], 12, 4);
      end;

end;


//Autofeed

procedure AutoFeed(P: Integer);
var
  K: Integer;
  UnitsToFeed: array of Integer;
begin
  UnitsToFeed := States.PlayerGetAllUnits(P);
  if Length(UnitsToFeed) <= 0 then
    Exit;
  for K := Low(UnitsToFeed) to High(UnitsToFeed) do
    Actions.UnitHungerSet(UnitsToFeed[K], States.UnitMaxHunger);
end;


procedure OnTick;
var 
  sw1, sw2, sw3, sw4, axe1, axe2, K: Integer;
begin

//Try to add new fish

  if States.GameTime mod FISH_TIME = 0 then 
    AddFish;

//Rebuild enemy base near the bridge

  Rebuild;

//AI Attacks

  if (States.GameTime >= 36000)
  and (AttackDelay = 0) then
    Attack;

  if AttackDelay > 0 then
    AttackDelay := AttackDelay - 1;

//Reinforcements

  if States.GameTime = 300 then
  begin
    axe1 := Actions.GiveGroup(2, 15, 17, 91, 4, Utils.CeilTo(8 * Balancer, 4), 6);
    axe2 := Actions.GiveGroup(2, 15, 177, 87, 4, Utils.CeilTo(8 * Balancer, 4), 6);
    sw1 := Actions.GiveGroup(2, 16, 15, 89, 4, Utils.CeilTo(10 * Balancer, 5), 6);
    sw2 := Actions.GiveGroup(2, 16, 175, 85, 4, Utils.CeilTo(10 * Balancer, 5), 6);
    Actions.GroupOrderLink(axe1, sw1);
    Actions.GroupOrderLink(axe2, sw2);
  end;
 
  if States.GameTime = 900 then
  begin
    sw3 := Actions.GiveGroup(2, 16, 138, 88, 5, Utils.CeilTo(20 * Balancer, 5), 5);
    sw4 := Actions.GiveGroup(2, 16, 60, 93, 3, Utils.CeilTo(20 * Balancer, 5), 5);
  end;

//Update coal and ores deposits

  if States.GameTime mod DEPOSITS_TIME = 0 then
    UpdateDeposits;

//Autofeed

  for I := 2 to 11 do
    if States.GameTime mod AUTOFEED_TIME = 5 * I then
      AutoFeed(I);

  for K := 0 to 1 do
    if SoundAllowed[K] then
    begin
      if (States.KaMRandom > 0.5)
      and (States.GameTime mod AMBIENT_TIME = 0) then
      begin
        Actions.PlayOGGAtLocation(K, 'Quack' + IntToStr(1 + States.KaMRandomI(3)), 0.75, 32, 60, 170);
        Actions.PlayOGGAtLocation(K, 'Quack' + IntToStr(1 + States.KaMRandomI(3)), 0.75, 32, 85, 170);
        Actions.PlayOGGAtLocation(K, 'Quack' + IntToStr(1 + States.KaMRandomI(3)), 0.75, 32, 110, 170);
        Actions.PlayOGGAtLocation(K, 'Quack' + IntToStr(1 + States.KaMRandomI(3)), 0.75, 32, 130, 170);
        Actions.PlayOGGAtLocation(K, 'Quack' + IntToStr(1 + States.KaMRandomI(3)), 0.75, 32, 26, 76);
        Actions.PlayOGGAtLocation(K, 'Quack' + IntToStr(1 + States.KaMRandomI(3)), 0.75, 32, 60, 65);
        Actions.PlayOGGAtLocation(K, 'Quack' + IntToStr(1 + States.KaMRandomI(3)), 0.75, 32, 100, 70);
        Actions.PlayOGGAtLocation(K, 'Quack' + IntToStr(1 + States.KaMRandomI(3)), 0.75, 32, 140, 70);
        Actions.PlayOGGAtLocation(K, 'Quack' + IntToStr(1 + States.KaMRandomI(3)), 0.75, 32, 180, 70);
        Actions.PlayOGGAtLocation(K, 'Quack' + IntToStr(1 + States.KaMRandomI(3)), 0.75, 32, 65, 32);
        Actions.PlayOGGAtLocation(K, 'Quack' + IntToStr(1 + States.KaMRandomI(3)), 0.75, 32, 150, 32);
      end;

      if (States.KaMRandom < 0.35)
      and (States.GameTime mod AMBIENT_TIME = 33) then
        if Length(Wolves) > 0 then
          for I := Low(Wolves) to High(Wolves) do
            if not States.UnitDead(Wolves[I]) then
              if States.KaMRandom < 0.25 then
                 Actions.PlayOGGAtLocation(K, 'Wolf' + IntToStr(1 + States.KaMRandomI(3)), Utils.MinS(States.KaMRandom, 0.35), 32, States.UnitPositionX(Wolves[I]), States.UnitPositionY(Wolves[I]));

    end;

end;