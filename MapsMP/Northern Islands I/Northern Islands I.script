const
  AUTOFEED_TIME = 6000;
  DEPOSITS_TIME = 1500;
  FISH_TIME = 450;
  AMBIENT_TIME = 250;


type
  RandomLoc = Record
    X, Y, Dir, SoldiersType: array[0..14] of Integer;
    CanBeTaken: array[0..14] of Boolean;
  end;
 

var 
  Fish, I, J, RebuildCount, AttacksCounter, AttackDelay: Integer;
  RebuildType, RebuildX, RebuildY: array of Integer;
  Wolves: array of Integer;
  SoundAllowed: array[0..1] of Boolean;
  LocsLeft, LocsRight, LocsTop: RandomLoc;


procedure FindWolves;
var
  aX, aY, W: Integer;
begin
  for aX := 1 to States.MapWidth - 1 do
    for aY := 1 to States.MapHeight - 1 do
    begin
      W := States.UnitAt(aX, aY);
      if W > 0 then
        if States.UnitType(W) = 30 then
        begin
          SetLength(Wolves, Length(Wolves) + 1);
          if Length(Wolves) > 0 then
            Wolves[Length(Wolves) - 1] := W;
        end;
    end;
end;


{$COMMAND sound: TurnSoundOnOff}


procedure TurnSoundOnOff(P, Value: Integer);
var
  aValue: Integer;
begin
  if not Utils.InRangeI(P, 0, 1) then
    Exit;
  aValue := Utils.EnsureRangeI(Value, 0, 1);
  SoundAllowed[P] := Boolean(aValue);
end;


//Adjust ware distribution for AI

procedure SetWareDistribution;
begin
  for I := 2 to 8 do
  begin
    Actions.PlayerWareDistribution(I, 6, 2, 5);
    Actions.PlayerWareDistribution(I, 6, 10, 5);
    Actions.PlayerWareDistribution(I, 5, 1, 5);
    Actions.PlayerWareDistribution(I, 5, 15, 5);
    Actions.PlayerWareDistribution(I, 5, 2, 5);
    Actions.PlayerWareDistribution(I, 5, 10, 5);
    Actions.PlayerWareDistribution(I, 2, 20, 5);
    Actions.PlayerWareDistribution(I, 2, 19, 5);
    Actions.PlayerWareDistribution(I, 9, 22, 5);
    Actions.PlayerWareDistribution(I, 9, 16, 5);
    Actions.PlayerWareDistribution(I, 9, 12, 5);
  end;
end;


//Prevents traffic jams

procedure BlockWares;
var
  H: array of Integer;
  K: Integer;
begin
  for I := 2 to 8 do
  begin
    H := States.PlayerGetAllHouses(I);

    if Length(H) <= 0 then
      Exit;

    for K := Low(H) to High(H) do
      if States.HouseType(H[K]) = 11 then
      begin
        Actions.HouseWareBlock(H[K], 0, True);
        Actions.HouseWareBlock(H[K], 1, True);
        Actions.HouseWareBlock(H[K], 2, True);
        Actions.HouseWareBlock(H[K], 3, True);
        Actions.HouseWareBlock(H[K], 4, True);
        Actions.HouseWareBlock(H[K], 5, True);
        Actions.HouseWareBlock(H[K], 6, True);
        Actions.HouseWareBlock(H[K], 9, True);
        Actions.HouseWareBlock(H[K], 11, True);
        Actions.HouseWareBlock(H[K], 14, True);
        Actions.HouseWareBlock(H[K], 15, True);
      end;
  end;
end;


//Adjust AI equip rates


procedure UpdateAIEquipRates;
begin
  case States.MissionDifficulty of

    mdEasy1: begin
               Actions.AIEquipRate(2, 0, 180);
               Actions.AIEquipRate(2, 1, 180);
               for I := 3 to 7 do
               begin
                 Actions.AIEquipRate(I, 0, 300);
                 Actions.AIEquipRate(I, 1, 300);
               end;
             end;

    mdNormal: begin
                Actions.AIEquipRate(2, 0, 150);
                Actions.AIEquipRate(2, 1, 150);
                for I := 3 to 7 do
                begin
                  Actions.AIEquipRate(I, 0, 250);
                  Actions.AIEquipRate(I, 1, 250);
                end;
              end;

    mdHard1: begin
               Actions.AIEquipRate(2, 0, 125);
               Actions.AIEquipRate(2, 1, 125);
               for I := 3 to 7 do
               begin
                 Actions.AIEquipRate(I, 0, 220);
                 Actions.AIEquipRate(I, 1, 220);
               end;
             end;

    mdHard2: begin
               Actions.AIEquipRate(2, 0, 100);
               Actions.AIEquipRate(2, 1, 100);
               for I := 3 to 7 do
               begin
                 Actions.AIEquipRate(I, 0, 180);
                 Actions.AIEquipRate(I, 1, 180);
               end;
             end;

    mdHard3: begin
               Actions.AIEquipRate(2, 0, 80);
               Actions.AIEquipRate(2, 1, 80);
               for I := 3 to 7 do
               begin
                 Actions.AIEquipRate(I, 0, 150);
                 Actions.AIEquipRate(I, 1, 150);
               end;
             end;

  end;
end;


